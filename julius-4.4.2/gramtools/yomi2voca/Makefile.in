SHELL=/bin/sh
RM=@RM@ -f
prefix=@prefix@
exec_prefix=@exec_prefix@
INSTALL=@INSTALL@

TARGET=yomi2voca.pl

all:
	chmod +x $(TARGET)

install: install.bin

install.bin:
	${INSTALL} -d @bindir@
	@INSTALL_PROGRAM@ $(TARGET) @bindir@

clean:
	echo nothing to do

distclean:
	$(RM) yomi2voca.pl
	$(RM) Makefile
